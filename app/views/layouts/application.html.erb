<!DOCTYPE html>
<html>
<head>
  <title>Maps</title>
  <%= include_gon %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<nav class="navbar navbar-inverse navbar-fixed-top">
  <div class="container">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <%= link_to 'R&P Db', cities_path, class:"navbar-brand" %>
    </div>
    <div id="navbar" class="collapse navbar-collapse">
      <ul class="nav navbar-nav">
        <li><%= link_to 'Cities', cities_path(@cities) %></li>
        <li><%= link_to 'Projects', projects_path(@projects) %></li>
        <li><%= link_to 'Developers', developers_path(@developers) %></li>
        <li><%= link_to 'Tenants', tenants_path(@tenants) %></li>
        <li><%= link_to 'News', articles_path(@articles) %></li>
      </ul>
       <div class="col-sm-4 col-md-4 pull-right">

        </div>        
    </div><!--/.nav-collapse -->
  </div>
</nav>

<%= yield %>

</body>
</html>
